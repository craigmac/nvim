{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT"
  },
  "diagnostics": {
    "disable": [
      "unused-local"
    ],
    "globals": [
      "vim",
      "describe",
      "it",
      "assert",
      "after_each",
      "before_each",
      "pending"
    ],
    "unusedLocalExclude": [
      "_*"
    ]
  },
  "workspace": {
    "library": [
      "$VIMRUNTIME"
    ],
    "checkThirdParty": false
  }
}
